{
    "name": "Search definition",
    "description": "DÃ©finition of words",
    "version":"1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"],
          "css" : ["style.css"],
          "run_at": "document_end"
        }
      ],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker" : "background.js"
    },
    "action":{
        "default_popup": "index.html",
        "default_title": "definition",
        "default_icons": "/img/icon_search.png"
        },
    "permissions": ["activeTab", "scripting"]
    }